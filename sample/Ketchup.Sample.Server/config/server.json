{
  "Logging": { //日志配置节点
    "LogLevel": { //默认筛选器
      "Default": "Debug", //默认日志类别
      "System": "Information" //日志类别为System
    },
    "Console": { //针对日志提供程序Console的配置
      "LogLevel": { //日志筛选器
        "Default": "Error", //默认日志类别//Error
        "LoggingSample.Program": "Debug" //日志类别为LoggingSample.Program的筛选器
      }
    }
  },
  "Server": {
    "Name": "sample",
    "Ip": "127.0.0.1",
    "Port": "5003",
    "Polly": "random"
  },

  "Consul": {
    "ConnectionString": "127.0.0.1:8500",
    "IsHealthCheck": true
  }
}
